@{
    ViewBag.Title = "Contact";
}

@model RaceDataHome.Models.ContactDetails

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>@ViewBag.Message</h2>
</hgroup>

<section class="contact">
    <p>
        <span>@Model.Name</span>
    </p>
    <p>
        <span class="label">Email:</span>
        <span>@Model.Email</span>
    </p>
</section>
